package arrays;

public class Array1 
{

	public static void main(String[] args) 
	{
		String name="Swati";
		System.out.println(name);
		
		String name2="Anand";
		System.out.println(name2);
		
		String[] stdnt=new String[6];
		stdnt[0]="Swati";
		stdnt[1]="Vinaya";
		stdnt[2]="Kalyani";
		stdnt[3]="Surajit";
		stdnt[4]="Arif";
		stdnt[5]="Ajashra";
		
		System.out.println(stdnt[0]);
	}

}
